<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="input.stdio.pe.kr.mainlauncherapp">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".IntentActivity" />
        <activity android:name=".IntentForResultActivity" />
        <activity android:name=".AsyncTaskActivity" />
        <activity android:name=".ProgressBarActivity" />
        <activity android:name=".CameraControlActivity" />
        <activity android:name=".GestureActivity" />
        <activity android:name=".ZoomTextActivity" />
        <activity android:name=".ListViewActivity" />
        <activity android:name=".SQLiteCheckActivity" />
        <activity android:name=".MemberActivity" />
        <activity android:name=".MemberInsertActivity"></activity>
    </application>

</manifest>